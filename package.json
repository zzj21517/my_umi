{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.2",
    "dva-model-creator": "^0.4.3",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.29.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rxjs": "^6.6.3",
    "umi": "^3.3.2",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  }
}
